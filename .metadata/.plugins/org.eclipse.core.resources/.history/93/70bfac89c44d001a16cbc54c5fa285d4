package SenderAndReceiver;
import java.util.ArrayList;

public class  Packet implements java.io.Serializable, MainMenu{

	private String sentence;
	private String name;
	private String number;
	private char gender;
	private int point;
	private int menuNo;
	private String newInfo;
	private boolean bool;
	private String adminID;
	private String adminPW;
	private boolean islogin;
	private String error;
	private boolean errorExist;
	private int infoMenu;
	private int pointNum;
	private int newPoint;
	private ArrayList<String> arrayList = new ArrayList<>();
	private Object[] array = new Object[12];
	
	
	public Packet(){
		this.adminID = null;
		this.adminPW = null;
		this.islogin = false;
		this.error = null;
		this.errorExist = false;
	}
	

	
	public ArrayList<String> getArrayList() {
		return arrayList;
	}



	public void setArrayList(ArrayList<String> arrayList) {
		this.arrayList = arrayList;
	}



	public int getNewPoint() {
		return newPoint;
	}

	public void setNewPoint(int newPoint) {
		this.newPoint = newPoint;
	}

	public int getPointNum() {
		return pointNum;
	}

	public void setPointNum(int pointNum) {
		this.pointNum = pointNum;
	}

	
	
	public int getInfoMenu() {
		return infoMenu;
	}


	public void setInfoMenu(int infoMenu) {
		this.infoMenu = infoMenu;
	}


	
	public String getAdminID() {
		return adminID;
	}

	public void setAdminID(String adminID) {
		this.adminID = adminID;
	}

	public String getAdminPW() {
		return adminPW;
	}

	public void setAdminPW(String adminPW) {
		this.adminPW = adminPW;
	}

	public boolean isIslogin() {
		return islogin;
	}

	public void setIslogin(boolean islogin) {
		this.islogin = islogin;
	}

	
	
	public boolean isErrorExist() {
		return errorExist;
	}

	public void setErrorExist(boolean errorExist) {
		this.errorExist = errorExist;
	}
	
	public String getError() {
		return error;
	}

	public void setError(String error) {
		array[ERRORNUM]=error;
		this.error = error;
	}

	
	public Object[] getArray() {
		return array;
	}

	public void setArray(Object[] array) {
		this.array = array;
	}

	public String getNewInfo() {
		return newInfo;
	}
	
	public void setSentence(String sentence) {
		array[SENTENCE]=sentence;
		this.sentence = sentence;
	}

	public void setName(String name) {
		array[NAME]=name;
		this.name = name;
	}


	public void setNumber(String number) {
		array[NUMBER]=number;
		this.number = number;
	}

	public void setGender(char gender) {
		array[GENDER]=gender;
		this.gender = gender;
	}

	public void setPoint(int point) {
		array[POINT]=point;
		this.point = point;
	}

	public void setMenuNo(int menuNo) {
		array[MENU]= menuNo;
		this.menuNo = menuNo;
	}

	public void setNewInfo(String newInfo) {
		array[NEWINFO]=newInfo;
		this.newInfo = newInfo;
	}

	public void setBool(boolean bool) {
		array[BOOL]=bool;
		this.bool = bool;
	}

	public String getSentence() {
		return sentence;
	}

	public String getName() {
		return name;
	}


	public String getNumber() {
		return number;
	}

	public char getGender() {
		return gender;
	}

	public int getPoint() {
		return point;
	}

	public int getMenuNo() {
		return menuNo;
	}

	public boolean isBool() {
		return bool;
	}

}
